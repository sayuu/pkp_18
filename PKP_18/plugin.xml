            id="jp.ac.hokudai.eng.complex.kussharo.sayuu.kp.testSiteView"
            name="学習タスク一覧ビュー"
            restorable="true">
      </view>
      <view
            category="jp.ac.hokudai.eng.complex.kussharo.sayuu.kp.views.category1"
            class="plugin.views.LogSiteView"
            id="jp.ac.hokudai.eng.complex.kussharo.sayuu.kp.logSiteView"
            name="履歴一覧ビュー"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
         <editor
         	id="jp.ac.hokudai.eng.complex.kussharo.sayuu.kp.formEditor"
         	class="plugin.formEditor.SampleFormEditor"
         	name="キーワードプログラミング設定画面"
         />
   </extension>
  	         			
   <extension
         point="org.eclipse.ui.startup">
      <startup class="plugin.activator.Activator"/>
   </extension>
   
