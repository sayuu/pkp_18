<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="jp.ac.hokudai.eng.complex.kussharo.sayuu.kp.JavaCompletionProposalComputer"
         name="jp.ac.hokudai.eng.complex.kussharo.sayuu.kp.CompletionProposal"
         point="org.eclipse.jdt.ui.javaCompletionProposalComputer">
      <javaCompletionProposalComputer
            activate="true"
            class="plugin.completionProposal.JavaCompletionProposalComputer">
      </javaCompletionProposalComputer>
   </extension>

   <extension
         point="org.eclipse.jdt.ui.javaCompletionProposalSorters">
      <proposalSorter
            class="plugin.completionProposal.ProposalSorter"
            id="jp.ac.hokudai.eng.complex.kussharo.sayuu.kp.proposalSorter"
            name="キーワードプログラミング">
      </proposalSorter>
   </extension>
 
   <extension
         point="org.eclipse.ui.commands">
         <category
         	name="jp.ac.hokudai.eng.complex.kussharo.sayuu.kp.commands.category1"
         	id="jp.ac.hokudai.eng.complex.kussharo.sayuu.kp.commands.category1">
         </category>
         <command
         	name="jp.ac.hokudai.eng.complex.kussharo.sayuu.kp.commands.command1"
         	categoryId="jp.ac.hokudai.eng.complex.kussharo.sayuu.kp.commands.category1"
         	id="jp.ac.hokudai.eng.complex.kussharo.sayuu.kp.commands.command1">
         </command>
         <command
         	name="jp.ac.hokudai.eng.complex.kussharo.sayuu.kp.commands.command2"
         	categoryId="jp.ac.hokudai.eng.complex.kussharo.sayuu.kp.commands.category1"
         	id="jp.ac.hokudai.eng.complex.kussharo.sayuu.kp.commands.command2">
         </command>
         
         <command
         	name="jp.ac.hokudai.eng.complex.kussharo.sayuu.kp.commands.command3"
         	categoryId="jp.ac.hokudai.eng.complex.kussharo.sayuu.kp.commands.category1"
         	id="jp.ac.hokudai.eng.complex.kussharo.sayuu.kp.commands.command3">
         </command>
         
         <command
         	name="jp.ac.hokudai.eng.complex.kussharo.sayuu.kp.commands.command4"
         	categoryId="jp.ac.hokudai.eng.complex.kussharo.sayuu.kp.commands.category1"
         	id="jp.ac.hokudai.eng.complex.kussharo.sayuu.kp.commands.command4">
         </command>
         
         <command
         	name="jp.ac.hokudai.eng.complex.kussharo.sayuu.kp.commands.command5"
         	categoryId="jp.ac.hokudai.eng.complex.kussharo.sayuu.kp.commands.category1"
         	id="jp.ac.hokudai.eng.complex.kussharo.sayuu.kp.commands.command5">
         </command>
   </extension>
   
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            commandId="jp.ac.hokudai.eng.complex.kussharo.sayuu.kp.commands.command1"
            class="plugin.handlers.saveTestSite.SaveTestSiteHandler">
      </handler>
      <handler
            commandId="jp.ac.hokudai.eng.complex.kussharo.sayuu.kp.commands.command2"
            class="plugin.handlers.saveTestSite.SaveSelectedSiteHandler">
      </handler>
      
      <handler
            commandId="jp.ac.hokudai.eng.complex.kussharo.sayuu.kp.commands.command3"
            class="plugin.handlers.getNumOfFunction.GetElementHandler">
      </handler>
      
      <handler
            commandId="jp.ac.hokudai.eng.complex.kussharo.sayuu.kp.commands.command4"
            class="plugin.handlers.getNumOfFunction.SearchHandler">
      </handler>
      
      <handler
            commandId="jp.ac.hokudai.eng.complex.kussharo.sayuu.kp.commands.command5"
            class="plugin.frequency.GetFrequencyHandler">
      </handler>
      
   </extension>
   
   <extension
         point="org.eclipse.ui.menus">
         <menuContribution
         	locationURI="popup:org.eclipse.ui.popup.any?after=additions">
         	<menu label="キーワードプログラミング">
         		<separator name="jp.ac.hokudai.eng.complex.kussharo.sayuu.kp.separator" visible="true" />
	         	<command
	         		commandId="jp.ac.hokudai.eng.complex.kussharo.sayuu.kp.commands.command1"
	         		id="jp.ac.hokudai.eng.complex.kussharo.sayuu.kp.menus.command1"
	         		icon="icons/add.gif"
	         		label="選択行を学習タスクとして追加">
	         		<visibleWhen>
	         			<with
	         				variable="activeMenuSelection">
	         				 <iterate ifEmpty="false">
							      <adapt type="org.eclipse.jface.text.ITextSelection">
      								</adapt>
    						</iterate>
	         			</with>
	         			<!--
	         			<with
	         				variable="activeEditorId">
	         				<equals
	         					value="org.eclipse.jdt.ui.CompilationUnitEditor">
	         				</equals>
	         			</with>
	         			-->
	         		</visibleWhen>
	         	</command>
	         	<command
	         		commandId="jp.ac.hokudai.eng.complex.kussharo.sayuu.kp.commands.command2"
	         		id="jp.ac.hokudai.eng.complex.kussharo.sayuu.kp.menus.command2"
	         		icon="icons/add.gif"
	         		label="選択要素以下を全て学習タスクとして追加">
	         		<visibleWhen>
	         			<with
	         				variable="activeMenuSelection">
	         				 <iterate ifEmpty="false">
							      <adapt type="org.eclipse.jdt.core.IJavaElement">
      								</adapt>
    						</iterate>
	         			</with>
	         		</visibleWhen>
	         	</command>
	         	
	         	<command
	         		commandId="jp.ac.hokudai.eng.complex.kussharo.sayuu.kp.commands.command3"
	         		id="jp.ac.hokudai.eng.complex.kussharo.sayuu.kp.menus.command3"
	         		icon="icons/add.gif"
	         		label="メソッド取得">
	         		
	         	</command>
	         	
	         	<command
	         		commandId="jp.ac.hokudai.eng.complex.kussharo.sayuu.kp.commands.command4"
	         		id="jp.ac.hokudai.eng.complex.kussharo.sayuu.kp.menus.command4"
	         		icon="icons/add.gif"
	         		label="メソッド検索">
	         		
	         	</command>
	         	
	         	<command
	         		commandId="jp.ac.hokudai.eng.complex.kussharo.sayuu.kp.commands.command5"
	         		id="jp.ac.hokudai.eng.complex.kussharo.sayuu.kp.menus.command5"
	         		icon="icons/add.gif"
	         		label="関数の頻度取得">
	         		
	         	</command>
         	</menu>
         </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="jp.ac.hokudai.eng.complex.kussharo.sayuu.kp.views.category1"
            name="キーワードプログラミング">
      </category>
      <view
            category="jp.ac.hokudai.eng.complex.kussharo.sayuu.kp.views.category1"
